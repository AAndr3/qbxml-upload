      <xsd:element name="authenticateResponse">
        <xsd:complexType>
          <xsd:sequence>
            <!-- array de 2 strings: [ticket, companyFileName|"nvu"] -->
            <xsd:element name="authenticateResult" type="tns:ArrayOfString"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>

      <xsd:complexType name="ArrayOfString">
        <xsd:sequence>
